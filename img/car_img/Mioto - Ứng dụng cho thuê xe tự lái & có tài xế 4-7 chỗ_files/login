document.domain="mioto.vn";var miotoLogin={};function getCookie(o){for(var e=o+"=",t=decodeURIComponent(document.cookie).split(";"),n=0;n<t.length;n++){for(var i=t[n];" "==i.charAt(0);)i=i.substring(1);if(0==i.indexOf(e))return i.substring(e.length,i.length)}return""}miotoLogin.onSubmit=function(){document.getElementById("miotologinform").target="miotologinframe",document.getElementById("miotologinform").submit()},miotoLogin.onComplete=function(o){if(miotoLogin.popup.close(),o.error>=0){if(null!=o.data&&1==o.data.value.isFirstLogin){var t=getCookie("_utm_src")?"web_directly":getCookie("_utm_src"),n=getCookie("_utm_ext")?"no_label":getCookie("_utm_ext");window.gtag('event',"ACCOUNT_REGISTER",{event_category:t,event_label:n,campaign:{source:t}});}window.location=window.location.href}else{var i=document.getElementById("miotologinmessage");i.innerHTML=e.errorMessage,i.style.display=""}},miotoLogin.openPopup=function(o){var e="https://accounts.mioto.vn/login/"+o;miotoLogin.popup=window.open(e,"","height=380,width=700,resizable=no,scrollbars=yes,toolbar=no,menubar=no,location=no,directories=no,status=yes")};