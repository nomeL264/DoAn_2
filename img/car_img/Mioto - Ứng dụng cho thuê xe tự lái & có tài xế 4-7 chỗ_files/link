document.domain="mioto.vn";var miotoLink={onSubmit:function(){document.getElementById("miotolinkform").target="miotolinkframe",document.getElementById("miotolinkform").submit()},onComplete:function(o){if(miotoLink.popup.close(),o.error>=0)window.location=window.location.href;else{var n=document.getElementById("miotolinkmessage");n.innerHTML=o.errorMessage,n.style.display=""}},openPopup:function(o){var n="https://accounts.mioto.vn/link/"+o;miotoLink.popup=window.open(n,"","height=380,width=700,resizable=no,scrollbars=yes,toolbar=no,menubar=no,location=no,directories=no,status=yes")}};